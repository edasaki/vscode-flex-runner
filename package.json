{
    "name": "flex-runner-vscode",
    "displayName": "flex-runner",
    "description": "Flexible run configurations & launch settings for your project building needs!",
    "version": "0.0.1",
    "publisher": "edasaki",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:flex-runner.run.config.1",
        "onCommand:flex-runner.run.config.last"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
               "command": "flex-runner.run.config.1",
               "title": "Flex: Run Configuration 1"
            },
            {
                "command": "flex-runner.run.config.last",
                "title": "Flex: Run Last Configuration"
            }
        ],
        "keybindings": [{
            "command": "flex-runner.run.config.last",
            "key": "ctrl-alt-r",
            "when": "editorFocus"
        }],
        "configuration": {
            "title": "",
            "type": "object",
            "properties": {
                
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}